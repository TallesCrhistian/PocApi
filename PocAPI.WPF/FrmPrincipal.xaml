<Window
    x:Class="PocAPI.WPF.FrmPrincipal"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:PocAPI.WPF"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="FrmPrincipal"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Grid>
        <Menu
            Name="MenPrincipal"
            Height="auto"
            VerticalAlignment="Top">
            <MenuItem Name="MeiCadastro" Header="Cadastros">
                <MenuItem Name="MeiCliente" Header="Cliente"  Click="MeiCliente_Click"/>
            </MenuItem>
        </Menu>
    </Grid>
</Window>
